https://trello.com/
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
const app = Vue.createApp({
    data: function () {
        return {
            primaryGoal: "Have a happy life",
            planLink: "https://trello.com/"
        }
    }
})

app.mount('#my-planner')
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="">
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    <script src="app.js"async defer></script>
</head>

<body>
    <section id="my-planner">
        <h2>My yearly plan</h2>
        <p>{{primaryGoal}}</p>
        <p>這樣work<a v-bind:href="planLink">Link</a></p>
        <p>這樣不會work<a href="{{planLink}}">Link</a></p>
    </section>
    
</body>

</html>
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
const app = Vue.createApp({
    data: function () {
        return {
            primaryGoal: "Have a happy life",
            planLink: "https://trello.com/"
        }
    },
    methods: {
        outputMessage: function () {
            const number1 = Math.random()
            if (number1 < 0.3) {
                return "work hard"
            } else {
                return "work harder"
            }
        }
    }
})

app.mount('#my-planner')
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="">
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    <script src="app.js" async defer></script>
</head>

<body>
    <section id="my-planner">
        <h2>My yearly plan</h2>
        <p>{{primaryGoal}}</p>
        <p>這樣work<a v-bind:href="planLink">Link</a></p>
        <p>這樣不會work<a href="{{planLink}}">Link</a></p>
        <p>直接呼叫函數:{{outputMessage()}}</p>
        <p>作一些計算{{2+3*5-4*4*4}}</p>
        <P>呼叫內建函數{{Math.random()}}</P>
    </section>

</body>

</html>
