Vue.createApp({
    data(){
        return {}
    },
    methods:{
        
    }
}).mount('#myapp');
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
Vue.createApp({
    data(){
        return {
            plans:[],
            value:''
        }
    },
    methods:{
        addPlan(){
            this.plans.push(this.value)
        }
    }
}).mount('#myapp');

~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
Vue.createApp({
    data(){
        return {
            plans:[],
            value:''
        }
    },
    methods:{
        addPlan(){
            this.plans.push(this.value)
        }
    }
}).mount('#myapp');
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="">
</head>

<body>
    <div id="myapp">
        <label for="plan">Plan</label>
        <input type="text" id="plan" v-model="value" />
        <button v-on:click="addPlan">Add Plan</button>
        <ul>
            <li v-for="p in plans">{{p}}</li>
        </ul>
    </div>

    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    <script src="app.js" async defer></script>
</body>

</html>
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
const {createApp} = Vue 
createApp({
    data(){
        return {
            plans:[],
            value:''
        }
    },
    methods:{
        addPlan(){
            this.plans.push(this.value)
            this.value=""
        }
    }
}).mount('#myapp');
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
index.html ==> copy from lab2
app.js

const {createApp} = Vue 
createApp({
    setup(){
        console.log("vue object會在此創建")
    }
}).mount('#myapp')
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
const {createApp, ref} = Vue 
createApp({
    setup(){
        console.log("vue object會在此創建")
        const plans = ref([])
        const value = ref('')
        function addPlan() {}
        return {
            plans:plans,
            value:value,
            addPlan:addPlan
        }
    }
}).mount('#myapp')
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
check f12, should no warning!!!

const {createApp, ref} = Vue 
createApp({
    setup(){
        console.log("vue object會在此創建")
        const plans = ref([])
        const value = ref('')
        setTimeout(()=>{
            plans.value.push("HAHA")
            setTimeout(()=>{
                plans.value.push("HAHAHA")
            },3000)
        },3000)
        function addPlan() {}
        return {
            plans:plans,
            value:value,
            addPlan:addPlan
        }
    }
}).mount('#myapp')

~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
const { createApp, ref } = Vue
createApp({
    setup() {
        console.log("vue object會在此創建")
        const plans = ref([])
        const value = ref('')
        function addPlan() {
            plans.value.push(value.value)
            value.value = ""
        }
        return {
            plans: plans,
            value: value,
            addPlan: addPlan
        }
    }
}).mount('#myapp')
